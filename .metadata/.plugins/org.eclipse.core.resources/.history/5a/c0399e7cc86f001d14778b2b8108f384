#ifndef INC_STATES_H_
#define INC_STATES_H_

typedef enum {
	INIT,
	WAITING,
	PRINTING
} sensorReaderState;

typedef enum {
	INIT,
	WAITING,
	PRINTING,
	INTERRUPTED
} commandParserState;

typedef enum {
	INIT,
	RUNNING,
} ledBlinkyState;

#endif /* INC_STATES_H_ */
