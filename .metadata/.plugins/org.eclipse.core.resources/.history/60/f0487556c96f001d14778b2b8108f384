#ifndef INC_STATES_H_
#define INC_STATES_H_

typedef enum {
	SENSOR_INIT,
	SENSOR_WAITING,
	SENSOR_PRINTING
} sensorReaderState;

typedef enum {
	PARSER_INIT,
	PARSER_WAITING,
	DATA_PRINTING,
	INTERRUPTED
} commandParserState;

typedef enum {
	LED_INIT,
	LED_BLINKING,
} ledBlinkyState;

#endif /* INC_STATES_H_ */
